tgls:Toggle(
    "AutoStomp Lmfao (currertly patched)",
    false,
    function(bool4)
while bool4 == true  do
local Players = game:GetService("Players")

local part = game:GetService("Players").LocalPlayer.Character.Head

local maxDistance = 10


	task.wait(0.6)
	local nearestPlayer, nearestDistance
	for _, player in pairs(Players:GetPlayers()) do
	    if player ~=  game:GetService("Players").LocalPlayer   and  player ~= nil then 
		local character = player.Character
		local distance = player:DistanceFromCharacter(part.Position)
		if not character or 
			distance > maxDistance or
			(nearestDistance and distance >= nearestDistance)
		then
			continue
		end
		nearestDistance = distance
		nearestPlayer = player
		end
	end
	print(nearestPlayer)
	pcall(function()
	    	l__ReplicatedStorage__5:WaitForChild("KillStreakEvent"):FireServer();
	l__ReplicatedStorage__5:WaitForChild("AttemptStomp"):FireServer(nearestPlayer.Character);
				l__ReplicatedStorage__5:WaitForChild("KillStreakEvent"):FireServer();
				end)
end
    end
)


     

