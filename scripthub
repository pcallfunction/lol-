lol:Button("small resize"
    ,function() 
    

local lplr = game.Players.LocalPlayer
local chara = lplr.Character
local hum = chara:FindFirstChildOfClass("Humanoid")

local Ignore = {
}

function emf(thing)
for i,v in pairs(chara:GetDescendants()) do
        if v and v.Name == "OriginalSize" and v.Parent ~= chara.Head then
            if table.find(Ignore, v.Parent) then
            else
            v:remove()
            end
        end
end

for i,v in pairs(chara:GetDescendants()) do
        if v and v:IsA("Attachment") and v.Parent ~= chara.Head then
            if v:FindFirstChild("OriginalPosition") then
            if table.find(Ignore, v.Parent) then
            else
            v.OriginalPosition:remove()
            end
            end
        end
end
wait(.2)
thing:remove()
end



emf(hum.HeadScale)
wait(.7)
emf(hum.BodyWidthScale)
wait(.7)
emf(hum.BodyDepthScale)
wait(.7)
emf(hum.BodyTypeScale)
wait(.7)
emf(hum.BodyHeightScale)
wait(.7)
emf(hum.BodyProportionScale)
end
)
